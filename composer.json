{
    "name": "fusic/limonite-sales",
    "description": "limonite-sales",
    "homepage": "http://fusic.co.jp",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "7.0.*",
        "cakephp/cakephp": "~3.4.2",
        "cakephp/migrations": "~1.0",
        "cakephp/plugin-installer": "*",
        "friendsofcake/search": "*",
        "fusic/encount": "1.*",
        "fusic/reincarnation": "1.*",
        "fusic/apollon": "1.*",
        "tutida/altair": "*",
        "tutida/pack": "*",
        "kozo/partial": "3.0.*@dev",
        "junkins/jp-chronos": "^0.0.7",
        "satthi/csv-combine-plugin-for-cakephp": "*",
        "satthi/phpexcelwrapper": "*",
        "kaihiro/optimistic-lock": "^1.0",
        "friendsofcake/bootstrap-ui": "*",
        "ginq/ginq": "^0.2.4",
        "k1low/ginq-csv": "^0.9.4"
    },
    "require-dev": {
        "cakephp/bake": "~1.1",
        "cakephp/cakephp-codesniffer": "dev-master",
        "cakephp/debug_kit": "~3.2",
        "digitalnature/php-ref": "dev-master",
        "friendsofphp/php-cs-fixer": "*",
        "phpmd/phpmd": "*",
        "phpunit/phpcov": "~3.0",
        "phpunit/phpunit": "~5.0",
        "psy/psysh": "@stable",
        "symfony/var-dumper": "*",
        "thomasbachem/php-short-array-syntax-converter": "dev-master",
        "k1low/po": "3.*"
    },
    "suggest": {
        "markstory/asset_compress": "An asset compression plugin which provides file concatenation and a flexible filter system for preprocessing and minification.",
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "App\\Console\\Installer::postInstall"
        ],
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}